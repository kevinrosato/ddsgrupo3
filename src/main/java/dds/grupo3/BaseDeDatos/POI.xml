<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="dds.grupo3.Interface.POI" table="POI" >
  	<id column="id" name="id" type="integer"/>
    <property name="nombre" />
    <property name="barrio" />
    <property name="calle" />
    <property name="callesPerpenIzq" />
    <property name="callesPerpenDer" />
    <property name="localidad" />
    <property name="provincia" />
    <property name="pais" />
    <property name="altura" />
    <property name="imagen" />
    
    <joined-subclass name="dds.grupo3.Interface.Local" extends="POI">
				<key column="id" />
				<property name="codigoPostal" />
   				<property name="departamento" />
    			<property name="piso" />
    			<property name="unidad" />
    			<property name="palabrasClaves" />
	</joined-subclass>
  </class>
</hibernate-mapping>

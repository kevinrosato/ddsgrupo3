<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="dds.grupo3.POIsSistem.Horario" table="Horario" >
  	<id column="horario_id" name="horario_id" type="integer"/>
  	<many-to-one name="Servicio" class="dds.grupo3.POIsSistem.Servicio" fetch="select">
            <column name="servicio_id" not-null="true" />
    </many-to-one>
    <property name="diaInicio" />
    <property name="diaFinal" />  
    <property name="horarioInicio" />  
    <property name="horarioCierre" />    
  </class>
</hibernate-mapping>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="dds.grupo3.POIsSistem.Servicio" table="Servicio" >
  	<id column="servicio_id" name="servicio_id" type="integer"/>
  	<property name="horario" />
    <property name="nombre" />
    <property name="radioCercania" />    
    <set name="Horario" table="Horario" inverse="true" lazy="true" fetch="select">
        <key><column name="servicio" not-null="true" /></key>
        <one-to-many class="dds.grupo3.POIsSistem.Horario" />
    </set>
  </class>
</hibernate-mapping>